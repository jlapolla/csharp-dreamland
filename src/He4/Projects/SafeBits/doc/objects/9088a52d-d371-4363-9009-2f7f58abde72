(max(values(D)) >= max(values(S))) ∧ ¬(min(values(S)) >= min(values(D)))
(DMAX           >= SMAX          ) ∧ ¬(SMIN           >= DMIN          )
(DMAX           >= SMAX          ) ∧ ¬(SMIN           >= DMIN          ) ∧ (s >= 0) ⇒ s ∈ values(D)
(DMAX >= SMAX) ∧ ¬(SMIN >= DMIN) ∧ (s >= 0) ⇒ s ∈ values(D)

